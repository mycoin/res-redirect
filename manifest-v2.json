{
   "name": "Res-Redirect",
   "version": "0.0.1",
   "description": "A static resources URL redirect tool.",
   "icons": {
      "128": "support/icon.png",
      "16": "support/icon.png",
      "32": "support/icon.png",
      "48": "support/icon.png"
   },
   "browser_action": {
      "default_icon": "support/icon.png",
      "default_title": "Res-Redirect",
      "default_popup": "popup.html"
   },
   "background": {
      "persistent": true,
      "scripts": [
         "dist/serviceWorker.js"
      ]
   },
   "manifest_version": 2,
   "options_page": "index.html",
   "permissions": [
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "storage",
      "unlimitedStorage",
      "<all_urls>"
   ]
}